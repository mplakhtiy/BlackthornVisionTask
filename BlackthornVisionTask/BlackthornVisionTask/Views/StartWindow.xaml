<Window x:Class="BlackthornVisionTask.Views.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StartWindow" Height="300" Width="300">
    <Grid>
        <Button  Command="{Binding SelectFolder,UpdateSourceTrigger=PropertyChanged}" Height="127" Width="292" Margin="0,0,0,142">Select Folder</Button>
        
        <Button  Command="{Binding FindDuplicatedFiles}" IsEnabled="{Binding CanExecuteFindDuplicatedFiles}" Height="127" Width="292" Margin="0,142,0,0" >Find duplicated files</Button>
        
    </Grid>
</Window>
